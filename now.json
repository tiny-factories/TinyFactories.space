{
  "version": 2,
    "name": "tinyfactories",
  "alias": ["tinyfactories.space"],
    "github": {
    "enabled": true
  },
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "views/index.html", "use": "@now/static" },
    { "src": "views/faq.html", "use": "@now/static" },
    { "src": "views/projects.html", "use": "@now/static" },
    { "src": "public/style.css", "use": "@now/static" },
    { "src": "public/opencall_style.css", "use": "@now/static" },
    { "src": "views/opencall.html", "use": "@now/static" },
    { "src": "public/opencall_scroll.js", "use": "@now/static" },
    { "src": "public/googleSheet.js", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/arena2slide(.*)",
      "status": 301,
      "headers": { "Location": "https://arena2slides.herokuapp.com/" }
    }
  ]
}
